<SettingMenu>:
    rows:10
    spacing:10






<Config>:

    size_hint_y:None
    height:50
    on_press:app.open_config() if root.configgeneral else root.NoTask()
    on_release:app.open_settings() if root.configgeneral else root.SelectConfig()
    Label:
        text:str(root.number) if root.number else ""
        center:self.parent.center
        font_size:self.height/5.5



<PopupNewUser>:
    rows:8
    GridLayout:
        size_hint_y:0.5
        cols:2
        padding:10
        spacing:20
        TextInput:
            id: UserName
            hint_text:'User Name'
            pos_hint: {'center_x': 0.5, 'center_y': 0.705}
            size_hint: 0.95, 0.5
        TextInput:
            id: Password
            hint_text:'Password'
            pos_hint: {'center_x': 0.5, 'center_y': 0.705}
            size_hint: 0.95, 0.5
            password: True

    GridLayout:
        cols:2
        size_hint_y:0.5
        padding:10
        spacing:20
        TextInput:
            id: Name
            hint_text:'First Name'
            pos_hint: {'center_x': 0.5, 'center_y': 0.705}
            size_hint: 0.95, 0.5

        TextInput:
            id: Family
            hint_text:'Family '
            pos_hint: {'center_x': 0.5, 'center_y': 0.705}
            size_hint: 0.95, 0.5
    GridLayout:
        cols:2
        size_hint_y:0.8
        padding:10
        spacing:20
        TextInput:
            id: Mobile
            hint_text:'Mobile Number'
            pos_hint: {'center_x': 0.5, 'center_y': 0.705}
            size_hint: 0.95, 0.5
        TextInput:
            id: Address
            hint_text:'Address'
            pos_hint: {'center_x': 0.5, 'center_y': 0.705}
            size_hint: 0.95, 0.5


    GridLayout:
        cols:6
        MCQLabelCheckBox:
            text:"Administrator"
            group: 'opts'
            adduser:True


        MCQLabelCheckBox:
            text:"Common"
            group: 'opts'
            adduser:True


        MCQLabelCheckBox:
            text:"Installer"
            group: 'opts'
            adduser:True
            size_hint_x:1
            opacity:0 if root.disableinstaller else 1
            disabled:1 if root.disableinstaller else 0
        MCQLabelCheckBox:
            text:"Supervisor"
            group: 'opts'
            adduser:True
            size_hint_x:1
            opacity:0 if root.disablesupervisor else 1
            disabled:1 if root.disablesupervisor else 0

    GridLayout:
        cols:4
        size_hint_y:0.3
        Label:
            size_hint_x:1.5
        Label:
            text:"Enable SMS"

        CheckBox:
            on_active:root.EnableSMS(self.active)
            size_hint_x:0.2
        Label:
            size_hint_x:1.5



    GridLayout:
        cols:3
        size_hint_y:0.5
        Label:
            size_hint_x:0.5

        Button:
            text:"Add User"
            on_press:root.AddUserToServer()
        Label:
            size_hint_x:0.5
    GridLayout:
        cols:3
        size_hint_y:0.2
        Label:
            size_hint_x:0.5

        Label:
            text:root.statetext
            color:root.colorgreen if root.writestate else root.colorerror

        Label:
            size_hint_x:0.5

<MCQCheckBox@CheckBox>:
    color: 1, 1, 1, 1
    size_hint: 0.3, 1


<MCQLabel@ButtonBehavior+Label>:
    text_size: self.size
    valign: 'center'
    font_size: '13sp'
    color: 1, 1, 1, 1

<MCQLabelCheckBox>:
    text: ''
    group: ''


    MCQCheckBox:
        id: cb
        group: root.group
        on_press:root.change_type(root.text)

    MCQLabel:

        text: root.text

