    GridLayout:
        cols:3
        size_hint_y:0.2
        Label:
            size_hint_x:0.5

        Label:
            text:root.statetext
            color:root.colorgreen if root.writestate else root.colorerror

        Label:
            size_hint_x:0.5


<PopupFaultList>:
    rows:8
    spacing:20
    padding:20

    GridLayout:
        cols:7
        size_hint_y:None
        size:50,40
        Label:
            size_hint_x:1
            text:"Date:"

        TextInput:
            id: year_start
            hint_text:'year'
        Label:
            size_hint_x:0.1
            text:"/"
        TextInput:

            id: mon_start
            hint_text:'mon'
        Label:
            size_hint_x:0.1
            text:"/"
        TextInput:

            id: day_start
            hint_text:'day'
        Label:
            size_hint_x:1





    GridLayout:
        size_hint_y:None
        size:50,40
        cols:3
        Label:
            size_hint_x:1
        Button:
            text:"Get Fault"
            on_press:root.GetFaults()
        Label:
            size_hint_x:1
    GridLayout:
        size_hint_y:None
        size:50,40
        cols:2
        Label:
            size_hint_x:1
            text:"Cnt Fault: "+str(root.cnt_fault)
            halign:"left"


    GridLayout:
        size_hint_y:None
        size:50,40
        cols:3
        Label:
            size_hint_x:1
        Button:
            on_press:root.SaveFaults()
            text:"Save Faults"
        Label:
            size_hint_x:1


